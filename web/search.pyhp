<html>
<body>
<ol>
<?

query_string = Page.args["q"]
global q
q = query_string[0]
query = lambda x: x["name"].find(q)>0 or x["desc"].find(q)>0


import metatorrent
MT = metatorrent.init()
for result in MT.search(query):
    Page.write("<li><a href=\"%s\">MAGNET</a> | %s | %s</li>"%(metatorrent.buildMagnet(result),result["name"],result["desc"]))


?>
</ol>
</body>
</html>
